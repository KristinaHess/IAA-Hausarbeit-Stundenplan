<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
  "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
  <package name="default" extends="tiles-default">
    <result-types>
      <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult"/>
    </result-types>
    <action name="editRoomPage">
      <result type="tiles">editRoomPage</result>
    </action>
    <action name="saveRoom" class="de.nak.iaaLecture.web.EditRoomAction" method="saveRoom">
      <result type="tiles">editRoomPage</result>
      <result name="input" type="tiles">editRoomPage</result>
    </action>
    <action name="listRoomsPage" class="de.nak.iaaLecture.web.ListRoomAction" method="listAll">
      <result type="tiles">listRoomsPage</result>
    </action>
  </package>
</struts>